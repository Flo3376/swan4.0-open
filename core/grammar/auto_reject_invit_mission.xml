<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<grammar version="1.0" xml:lang="fr-FR" mode="voice" root="reject_invit_mission" xmlns="http://www.w3.org/2001/06/grammar" tag-format="semantics/1.0">
  <rule id="reject_invit_mission" scope="public">
    <tag>out.action=new Object();</tag>
    <item>Swan</item>
    <one-of>
      <item>refuse la mission</item>
      <item>refuse l'invitation</item>
    </one-of>
    <tag>out.action.action="reject_invit_mission";</tag>
  </rule>
</grammar>