<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<grammar version="1.0" xml:lang="fr-FR" mode="voice" root="call_station_exit" xmlns="http://www.w3.org/2001/06/grammar" tag-format="semantics/1.0">
  <rule id="call_station_exit" scope="public">
    <tag>out.action=new Object();</tag>
    <item>Swan</item>
    <one-of>
      <item>Contacte la station pour le décollage</item>
      <item>Demande l'autorisation de décoller</item>
      <item>Appelle la station</item>
      <item>on y va</item>
      <item>on décolle</item>
    </one-of>
    <tag>out.action.action="call_station_exit";</tag>
  </rule>
</grammar>